<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <servlet>
    <servlet-name>ANTLRServlet</servlet-name>
    <servlet-class>com.fan.ANTLR.servlet.ANTLRServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ANTLRServlet</servlet-name>
    <url-pattern>/sqlEditor</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ParseTreeServlet</servlet-name>
    <servlet-class>com.fan.gui.servlet.ParseTreeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ParseTreeServlet</servlet-name>
    <url-pattern>/tree</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getDBServlet</servlet-name>
    <servlet-class>com.fan.db.servlet.getDBServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getDBServlet</servlet-name>
    <url-pattern>/getDB</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getTableServlet</servlet-name>
    <servlet-class>com.fan.db.servlet.getTableServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getTableServlet</servlet-name>
    <url-pattern>/getTable</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getColumnServlet</servlet-name>
    <servlet-class>com.fan.db.servlet.getColumnServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getColumnServlet</servlet-name>
    <url-pattern>/getColumn</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>sessionServlet</servlet-name>
    <servlet-class>com.fan.session.servlet.sessionServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>sessionServlet</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>resultServlet</servlet-name>
    <servlet-class>com.fan.result.servlet.resultServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>resultServlet</servlet-name>
    <url-pattern>/result</url-pattern>
  </servlet-mapping>

</web-app>