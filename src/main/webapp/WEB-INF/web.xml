<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <servlet>
    <servlet-name>MainPageServlet</servlet-name>
    <servlet-class>com.fan.MainPageServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MainPageServlet</servlet-name>
    <url-pattern>/sqlEditor</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ParseTreeServlet</servlet-name>
    <servlet-class>com.fan.gui.ParseTreeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ParseTreeServlet</servlet-name>
    <url-pattern>/tree</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getDBServlet</servlet-name>
    <servlet-class>com.fan.db.getDBServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getDBServlet</servlet-name>
    <url-pattern>/getDB</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getTableServlet</servlet-name>
    <servlet-class>com.fan.db.getTableServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getTableServlet</servlet-name>
    <url-pattern>/getTable</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getColumnServlet</servlet-name>
    <servlet-class>com.fan.db.getColumnServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getColumnServlet</servlet-name>
    <url-pattern>/getColumn</url-pattern>
  </servlet-mapping>

</web-app>